    },
    "require": {
        "zendframework/zend-db": "^2.0.1",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-stdlib": "^3.0.1",
        "phpunit/phpunit": "^4.0",
        "phpunit/phpunit-skeleton-generator": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "zaboy\\res\\": "src/"
