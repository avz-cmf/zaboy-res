{
    "name": "avz-cmf/zaboy-res",
    "description": "Data store as REST resource",
    "type": "library",
    "license": "GPL-3.0",
    "keywords": [
        "php",
        "rest",
        "data store",
        "resource"
    ],
    "homepage": "https://github.com/avz-cmf/zaboy-res",
    "support": {
